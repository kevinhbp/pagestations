<?xml version="1.0" encoding="UTF-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape>
      <solid android:color="@color/text_white_2" />
      <corners
        android:topLeftRadius="@dimen/fo_radius_default"
        android:topRightRadius="@dimen/fo_radius_default" />
    </shape>
  </item>
  <item
    android:left="1dp"
    android:right="1dp"
    android:top="1dp">
    <shape>
      <solid android:color="@color/white" />
      <corners
        android:topLeftRadius="@dimen/fo_radius_default"
        android:topRightRadius="@dimen/fo_radius_default" />
    </shape>
  </item>
</layer-list>